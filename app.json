{
  "expo": {
    "name": "LSAT Logic Daily",
    "slug": "lsat-logic-daily",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a1a2e"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1a1a2e"
      },
      "package": "com.yourcompany.lsatlogic",
      "versionCode": 1,
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE"
      ],
      "playStoreUrl": "https://play.google.com/store/apps/details?id=com.yourcompany.lsatlogic"
    },
    "plugins": [
      [
        "expo-ads-admob",
        {
          "androidAppId": "ca-app-pub-YOUR_ANDROID_APP_ID"
        }
      ],
      "expo-notifications"
    ],
    "extra": {
      "eas": {
        "projectId": "aace35ab-62b2-4c3a-bdc3-ad02d0a49add"
      }
    }
  }
}
